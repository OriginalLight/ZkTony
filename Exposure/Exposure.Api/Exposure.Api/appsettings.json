{
  "DataBase": "Exposure.db",
  "AllowedHosts": "*",
  "SerialPort": {
    "Com1": "COM4",
    "Com2": "COM3"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log.txt",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 7
        }
      }
    ]
  }
}
